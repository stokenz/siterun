{
  "expo": {
    "name": "SiteRun",
    "slug": "siterun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-icon.png",
    "scheme": "siterun",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jpbow.siterun",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/images/logo-icon.png",
        "imageWidth": 1024,
        "resizeMode": "contain",
        "backgroundColor": "#228be6"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-icon.png",
        "backgroundColor": "#228be6"
      },
      "splash": {
        "image": "./assets/images/logo-icon.png",
        "imageWidth": 1024,
        "resizeMode": "contain",
        "backgroundColor": "#228be6"
      }
    },
    "plugins": ["expo-router", "expo-splash-screen", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c9f0f2ab-2248-4617-a963-149e46d19bb9"
      }
    }
  }
}
